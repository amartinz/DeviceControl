<resources>
    <!-- theme color -->
    <attr name="themeColor" format="reference|color" />
    <attr name="themeColorInvert" format="reference|color" />
    <attr name="themeMenu" format="reference|color" />
    <attr name="themeMenuFooter" format="reference|color" />
    <!-- awesome preference -->
    <declare-styleable name="AwesomePreference">
        <attr name="filePath" format="reference" />
        <attr name="filePathList" format="reference" />
        <attr name="fileValue" format="reference" />
        <attr name="category" format="string" />
        <attr name="startup" format="boolean" />
        <attr name="multifile" format="boolean" />
        <attr name="valueChecked" format="string" />
        <attr name="valueNotChecked" format="string" />
    </declare-styleable>
    <!-- seekbar preference -->
    <declare-styleable name="SeekBarPreference">
        <attr name="min" format="integer" />
        <attr name="unitsLeft" format="string" />
        <attr name="unitsRight" format="string" />
    </declare-styleable>
    <!-- Card Views -->
    <declare-styleable name="CardTitleView">
        <attr name="icon" />
        <attr name="iconTint" format="color|reference" />
        <attr name="title" />
    </declare-styleable>
</resources>
